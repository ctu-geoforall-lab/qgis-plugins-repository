<?xml version="1.0"?>
<plugins>
    <pyqgis_plugin name="VFK Plugin" version="2.0.0">
        <description>Plugin umožňující práci s daty českého katastru
        nemovitostí ve formátu VFK. Plugin podporuje vyhledávání
        parcel, budov, vlastníků a jednotek. Výsledky vyhledávání je
        následně možné exportovat a zobrazit v mapě.</description>
        <homepage>http://freegis.fsv.cvut.cz/gwiki/VFK_/_QGIS_plugin</homepage>
        <qgis_minimum_version>2.8</qgis_minimum_version>
        <file_name>qgis-vfk-plugin-release-2_0.zip</file_name>
        <author_name>Petrasova, Petras, Bambula, Landa (CTU GeoForAll Lab)</author_name>
        <download_url>https://codeload.github.com/ctu-geoforall-lab/qgis-vfk-plugin/zip/release-2_0</download_url>
    </pyqgis_plugin>
    <pyqgis_plugin name="GPS Position Lag Correction" version="0.9">
        <description>Aerial Data Shift</description>
        <homepage>https://ctu-geoforall-lab.github.io/qgis-position-lag-correction-plugin/</homepage>
        <qgis_minimum_version>2.8</qgis_minimum_version>
        <file_name>qgis-position-lag-correction-plugin-master.zip</file_name>
        <author_name>Pesek, Landa (CTU GeoForAll Lab)</author_name>
        <download_url>https://codeload.github.com/ctu-geoforall-lab/qgis-position-lag-correction-plugin/zip/master</download_url>
    </pyqgis_plugin>
    <pyqgis_plugin name="Map Corners Coordinates" version="0.1">
        <description>QGIS plugin for saving map canvas coordinates into a text file</description>
        <homepage>https://ctu-geoforall-lab.github.io/qgis-map-coords-plugin</homepage>
        <qgis_minimum_version>2.8</qgis_minimum_version>
        <file_name>qgis-map-coords-plugin-master.zip</file_name>
        <author_name>Kulovana, Kala, Landa (CTU GeoForAll Lab)</author_name>
        <download_url>https://codeload.github.com/ctu-geoforall-lab/qgis-map-coords-plugin/zip/master</download_url>
    </pyqgis_plugin>
    <pyqgis_plugin name="RUIAN Plugin" version="0.1">
        <description>Nastroj pro stahovani datovych vrstev RUIAN</description>
        <homepage>https://ctu-geoforall-lab.github.io/qgis-ruian-plugin</homepage>
        <qgis_minimum_version>2.8</qgis_minimum_version>
        <file_name>qgis-ruian-master.zip</file_name>
        <author_name>Klima, Streda, Gajzler, Landa (CTU GeoForAll Lab)</author_name>
        <download_url>https://codeload.github.com/ctu-geoforall-lab/qgis-ruian-plugin/zip/master</download_url>
    </pyqgis_plugin>
</plugins>
